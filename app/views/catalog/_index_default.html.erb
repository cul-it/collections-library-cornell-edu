<% real_page = render_index_field_value :document => document, :field => 'image_n_tesim' %>	
<% page_turner = pubstmt_to_id(render_index_field_value :document => document, :field => 'pubstmt_idno_tesim') %>

  <div class="data-block">
    <div class="blacklight-author_display"><%= render_index_field_value :document => document, :field => 'author_tesim' %></div>
    <div class="blacklight-publisher_display"><%= render_index_field_value :document => document, :field => 'pubplace_tesim' %>, <%= render_index_field_value :document => document, :field => 'publisher_tesim' %>
      <span class="blacklight-publisher_uniform_display"><%= render_index_field_value :document => document, :field => 'pubdate_tesim' %></span>
    </div>
    <div class="blacklight-page_turner_link" data-no-turbolink>
    <% unless not_digitized?(page_turner) %>
      <a href="http://reader.library.cornell.edu/docviewer/digital?id=<%= page_turner %>">Read document</a> | <a href="<%= ENV['STORAGE_URL'] %><%= page_turner %>/datastreams/pdf/content">Download PDF</a>
      <% end %>
    </div>
  </div>

